<div class="container nav">
    <div class="row">
        <div class="twelve columns navbar">
            <%= link_to image_tag("logo1.png", class: "logo basic-link"), root_path %>
            <div class="right">
                <% if user_signed_in? %>
                    <%= link_to "Create New Project", new_project_path, class: "button-primary"%>
                    <%= link_to "Logout", destroy_user_session_path, method: :delete  %>
                <% else %>
                    <%= link_to "Login", new_user_session_path%>
                    <%= link_to "Join", new_user_registration_path, class: "button-primary"%>
                <% end %>
            </div>
        </div>
    </div>
</div>
